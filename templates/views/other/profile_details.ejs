                        JSON.stringify({ fieldsArr: fieldsArr, pageNo: currentPageNo })

                })
                    .then(response => response.json())
                    .then(json => console.log(json))
                    .catch(err => console.log(err));

                // var js={
                //     name:value
                // }
                // alert(JSON.stringify(js))
            }
            profile_details_card[currentPageNo].style.display = "none"
            profile_details_card[currentPageNo + 1].style.display = "block"

            if (currentPageNo == 6) {
                submitBtn.style.display = "block"
                alert(currentPageNo)
            }
        }

        function previousPage(currentPageNo) {
            //alert(currentPageNo)


            profile_details_card[currentPageNo].style.display = "none"
            profile_details_card[currentPageNo - 1].style.display = "block"
            if (currentPageNo == 7) {
                submitBtn.style.display = "none"
            }

        }

    </script>
</body>

</html>



