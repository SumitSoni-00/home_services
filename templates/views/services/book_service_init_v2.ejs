        <script>
            const firstName = document.getElementById("first_name")
            const lastName = document.getElementById("last_name")
            const contactPhone = document.getElementById("contact_phone")
            const alternatePhone = document.getElementById("alternate_phone")
            const addressEL = document.getElementById("address")
            const cityEL = document.getElementById("city")
            const stateEL = document.getElementById("state")
            const pinCodeEl = document.getElementById("pinCode")
            const serviceSubmitForm = document.getElementById("service_submit_form")
            serviceSubmitForm.onsubmit = () => {

                if (globalSelected === null) {
                    alert(globalSelected)
                    let isNamesValid = namesValid()
                    let isPhonesValid = phonesValid()
                    let isAddressesValid = addressesValid()
                    let isFormValid = isNamesValid && isPhonesValid && isAddressesValid

                    return isFormValid
                } else {
                    let isNamesValid = namesValid()
                    let isPhonesValid = phonesValid()

                    let isFormValid = isNamesValid && isPhonesValid && isAddressesValid

                    return isFormValid
                }

            }

            function addressesValid() {
                let valid = true
                if (!isCorrectAddress(addressEL.value)) {
                    valid = false

                    showError(addressEL)
                } else {
                    showSuccess(addressEL)
                }

                if (stateEL.selectedIndex === 0) {
                    valid = false

                    showError(stateEL)
                } else {
                    showSuccess(stateEL)
                }

                if (!isPinCodeValid(pinCodeEl.value)) {
                    valid = false
                    showError(pinCodeEl)
                } else {
                    showSuccess(pinCodeEl)

                }


                if (!isCorrectCity(cityEL.value)) {
                    valid = false
                    showError(cityEL)
                } else {
                    showSuccess(cityEL)

                }

                return valid
            }


            function isPinCodeValid(value) {
                var regex = /^([1-9]{1}[0-9]{5}|[1-9]{1}[0-9]{2}\s{0,1}[0-9]{3})$/
                return regex.test(value.trim());
            }


            function namesValid() {
                // alert("p")
                let valid = true
                if (!isCorrectName(firstName.value)) {
                    valid = false

                    showError(firstName)
                } else {
                    showSuccess(firstName)
                }
                if (!isCorrectName(lastName.value)) {
                    valid = false
                    showError(lastName)
                } else {
                    showSuccess(lastName)
                }

                return valid
            }

            function phonesValid() {
                // alert("p")
                let valid = true
                if (!isCorrectPhone(contactPhone.value)) {
                    valid = false

                    showError(contactPhone)
                } else {
                    showSuccess(contactPhone)
                }
                if (alternatePhone.value.trim().length !== 0) {
                    if (!isCorrectPhone(alternatePhone.value)) {
                        valid = false
                        showError(alternatePhone)
                    } else {
                        // alert(alternatePhone.value.trim().length)
                        showSuccess(alternatePhone)
                    }
                } else {
                    showSuccess(alternatePhone)
                }


                return valid
            }


            // window.onload=()=>{
            //     var regex=/^([1-9]{1}[0-9]{5}|[1-9]{1}[0-9]{2}\s{0,1}[0-9]{3})$/
            //     alert( regex.test("482 002"))
            // }


            function isCorrectName(value) {
                var regex = /^[a-zA-Z ]{2,30}$/;

                return regex.test(value.trim());
                // return value.trim().length==0?false:true
            }

            function isCorrectCity(value) {
                var regex = /^[a-zA-Z ]{2,100}$/;

                return regex.test(value.trim());
                // return value.trim().length==0?false:true
            }

            // window.onload=()=>{
            //     var regex = /^[a-zA-Z,. ]{2,30}$/;


            // }

            function isCorrectAddress(value) {
                var regex = /^[a-zA-Z0-9,.() ]{2,100}$/;
                return regex.test(value.trim());
            }




            function isCorrectPhone(value) {
                var regex = /^[6-9]{1}[0-9]{9}$/;

                return regex.test(value.trim());
                // return value.trim().length==0?false:true
            }

            function showError(inputEl) {
                inputEl.classList.remove("valid_field")
                inputEl.classList.add("invalid_field")

            }

            function showSuccess(inputEl) {
                inputEl.classList.remove("invalid_field")
                //inputEl.classList.add("valid_field")

            }
        </script>

        <script>

            const addressFields = document.getElementsByClassName("address_fields")
            const saveAddressBtn = document.getElementById("save_address_btn")
            const existingFlag = document.getElementById("existing_flag")
            const saveAddressField = document.getElementById("save_address")
            function saveAddress(event) {
                alert(saveAddressField.value)
                saveAddressField.setAttribute("value", "true")
                return



                //alert(addressFields)
                if (!addressesValid()) {
                    return
                } event.target.setAttribute("disabled", "disabled")
                const addressData = {
                    address: addressFields[0].value,
                    city: addressFields[1].value,
                    state: addressFields[2].value,
                    pinCode: addressFields[3].value

                }
                //alert(JSON.stringify(addressData))

                fetch('/customer/save_address', {
                    method: "POST",
                    headers: { "Content-type": "application/json;charset=UTF-8" },
                    body: JSON.stringify(addressData)

                })
                    .then(response => response.json())
                    .then((json) => {
                        alert(json)

                        selectedAddressFields[0].setAttribute("value", addressData.address)
                        selectedAddressFields[1].setAttribute("value", addressData.city)
                        selectedAddressFields[2].setAttribute("value", addressData.state)
                        selectedAddressFields[3].setAttribute("value", addressData.pinCode)
                        existingFlag.value = "true"
                        event.target.style.display = "none"
                        for (let index = 0; index < 4; index++) {
                            let element = addressFields[index];
                            element.setAttribute("disabled", "disabled")

                        }

                    })
                    .catch((err) => {
                        alert(err)
                    });
            }
        </script>

        <script>
            const selectedAddressFields = document.getElementsByClassName("selected_address_fields")
            const addressBoxColl = document.getElementsByClassName("address_box")
            let globalSelected = null;
            function selectAddress(addressBoxIndex) {
                //alert(addressBoxIndex)
                if (addressBoxColl[addressBoxIndex].classList.contains("selected_class")) {
                    unSelectelectAddressBox(addressBoxIndex)
                    globalSelected = null
                    for (let index = 0; index < 4; index++) {
                        let element = addressFields[index];
                        element.removeAttribute("disabled")

                    }

                    try {
                        saveAddressBtn.removeAttribute("disabled")
                    } catch (error) {
                        //alert(error)

                    }
                    existingFlag.removeAttribute("value")



                } else {
                    if (globalSelected !== null) {
                        // alert(globalSelected)
                        unSelectelectAddressBox(globalSelected)
                    }
                    globalSelected = addressBoxIndex
                    //alert(globalSelected)

                    selectAddressBox(addressBoxIndex)
                }



            }


            function unSelectelectAddressBox(addressBoxIndex) {
                addressBoxColl[addressBoxIndex].classList.remove("selected_class")
                addressBoxColl[addressBoxIndex].getElementsByClassName("select_btn")[0].innerHTML = `<i class="fas fa-plus"></i>&nbsp;| Select`
                addressBoxColl[addressBoxIndex].getElementsByClassName("select_btn")[0].classList.remove("active_btn")
                selectedAddressFields[0].removeAttribute("value")
                selectedAddressFields[1].removeAttribute("value")
                selectedAddressFields[2].removeAttribute("value")
                selectedAddressFields[3].removeAttribute("value")



            }

            function selectAddressBox(addressBoxIndex) {
                const address = addressBoxColl[addressBoxIndex].getElementsByClassName("address_box_street")[0].innerText
                const city = addressBoxColl[addressBoxIndex].getElementsByClassName("address_box_items")[0].innerText
                const state = addressBoxColl[addressBoxIndex].getElementsByClassName("address_box_items")[1].innerText
                const pinCode = addressBoxColl[addressBoxIndex].getElementsByClassName("address_box_items")[2].innerText

                //alert(`${address},${city},${state},${pinCode}`)
                addressBoxColl[addressBoxIndex].classList.add("selected_class")
                addressBoxColl[addressBoxIndex].getElementsByClassName("select_btn")[0].innerHTML = `<i class="far fa-check-circle"></i>&nbsp;| Unselect`
                addressBoxColl[addressBoxIndex].getElementsByClassName("select_btn")[0].classList.add("active_btn")
                selectedAddressFields[0].setAttribute("value", address)
                selectedAddressFields[1].setAttribute("value", city)
                selectedAddressFields[2].setAttribute("value", state)
                selectedAddressFields[3].setAttribute("value", pinCode)
                saveAddressField.removeAttribute("value")
                for (let index = 0; index < 4; index++) {
                    let element = addressFields[index];
                    element.classList.remove("invalid_field")
                    element.setAttribute("disabled", "disabled")
                    //element.value = ""



                }

                try {
                    saveAddressBtn.setAttribute("disabled", "disabled")
                } catch (error) {
                    // alert(error)
                }
                existingFlag.value = "true"
            }
        </script>


