        function readURL(input) {
            if (input.files && input.files[0]) {
                var reader = new FileReader();
                reader.onload = function (e) {
                    $('#imagePreview').css('background-image', 'url(' + e.target.result + ')');
                    $('#imagePreview').hide();
                    $('#imagePreview').fadeIn(650);
                }
                reader.readAsDataURL(input.files[0]);
            }
        }
        $("#imageUpload").change(function (event) {
            //alert(this)
            handleImageUpload(event, this)
            //readURL(this);
        });

        const handleImageUpload = (event, inputFileEl) => {
            //alert("hello")
            const files = event.target.files
            const formData = new FormData()
            //alert(`${files[0]} IDD`)
            formData.append('myImage', files[0])
            formData.append('pageNo', 4)
            //alert(formData)
            fetch('/employee/submit_profile_details/upload_pic', {
                method: 'POST',
                //headers: { "Content-type": "application/json;charset=UTF-8" },
                body: formData//JSON.stringify({name:"sum",pass:"5797"})//formData
            })
                .then(response => response.json())
                .then(data => {
                    //console.log(data)
                    readURL(inputFileEl)
                    alert(data)
                })
                .catch(error => {
                    //console.error(error)
